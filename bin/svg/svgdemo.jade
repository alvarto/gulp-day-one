h1 SVG Demo
- var fileAddr = 'resource/img/' + directory + '/v/svg.svg'
h2 File in 
	code= fileAddr
each file in arr
	-var symbol = file.replace(".svg", "")
	div(style={float: "left", "margin-right": '20px', 'text-align': 'center'})
		svg(width=180 height=180)
			use(xlink:href=fileAddr + "#"+symbol)
		p: code= '#' + symbol
